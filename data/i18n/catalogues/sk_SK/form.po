msgid "This value is required."
msgstr "Túto hodnotu je nutné zadať."

msgid "This value is invalid."
msgstr "Táto hodnota nie je platná."

msgid "Value \"%value%\" is forbidden."
msgstr "Zakázaná hodnota  \"% hodnota% \"."

msgid "Selected choice is invalid."
msgstr "Vybraná voľba je neplatná."

msgid "At least %min% values must be selected (%count% values selected)."
msgstr ""
"Minimálne musí byť hodnoty v% min% byť vybraný (hodnoty v% počet% vybratý)."

msgid "At most %max% values must be selected (%count% values selected)."
msgstr "Na väčšine % max % musí byť vybrané (% count% vybrané hodnoty)."

msgid "Date is required."
msgstr "Zadajte dátum."

msgid "\"%value%\" is invalid date value."
msgstr "\"% value% \" je neplatný dátum."

msgid "\"%value%\" does not match the date format (%date_format%)."
msgstr "\"%value%\" nezodpovedá formátu dátumu (%date_format%)."

msgid "The date must be before %max%."
msgstr "Dátum musí byť pred %max%."

msgid "The date must be after %min%."
msgstr "Dátum musí byť po %min%. "

msgid "The date cannot be in the future."
msgstr "Dátum nemôže byť v budúcnosti."

msgid "The date cannot be in the past."
msgstr "Dátum nemôže byť v minulosti."

msgid "The time interval between the two dates is too short."
msgstr "Časový interval medzi dvoma dátumami je príliš krátký."

msgid "The time interval between the two dates is too long."
msgstr "Časový interval medzi dvoma dátumami je príliš dlhý."

msgid "The start date must be prior to the end date."
msgstr "Dátum začatia musí byť pred dátumom ukončenia."

msgid "Unexpected extra form field named \"%field%\"."
msgstr "Neočakávaný ďalšie pole formulára s názvom  \"%field% \"."

msgid ""
"The form submission cannot be processed. It probably means that you have "
"uploaded a file that is too big."
msgstr ""
"Odoslanie formulára nemôže byť spracované. Pravdepodobne to znamená, že ste "
"nahrali súbor, ktorý je príliš veľký."

msgid "Time is required."
msgstr "Zadajte čas."

msgid "\"%value%\" is invalid time value."
msgstr "\"%value%\" je neplatný časový údaj."

msgid "\"%value%\" does not match the time format (%time_format%)."
msgstr "\"%value%\" nie je vo formáte času (%time_format%)."

msgid "Select a file to be uploaded."
msgstr "Vyberte súbor pre nahratie."

msgid "Invalid file uploaded."
msgstr "Neplatný nahraný súbor."

msgid "File is too large (maximum is %max_size% bytes)."
msgstr "Súbor je príliš velký  (maximum je %max_size% bytes)."

msgid "Invalid mime type (%mime_type%)."
msgstr "Neplatný typ MIME (% mime_type%)."

msgid "The uploaded file was only partially uploaded."
msgstr "Nahrávaný súbor bol nahraný iba čiastočne."

msgid "Missing a temporary folder."
msgstr "Chýba dočasný adresár."

msgid "Failed to write file to disk."
msgstr "Súbor sa nepodarilo sa zapísať na disk."

msgid "File upload stopped by extension."
msgstr "Upload súboru zastavilo rozšírenie."

msgid "Select an image to be uploaded."
msgstr "Vyberte obrázok k nahratiu."

msgid ""
"Image dimensions %width%x%height%px are too small (minimum is %min_width%x"
"%min_height%px)."
msgstr ""
"Rozmery obrázku %width%x%height%px  sú príliš malé  (minimumje %min_width%x"
"%min_height%px)."

msgid ""
"Image dimensions %width%x%height%px are too large (maximum is %max_width%x"
"%max_height%px)."
msgstr ""
"Rozmery obrázku %width%x%height%px  sú príliš velké  (maximum je %max_width%x"
"%max_height%px)."

msgid "\"%value%\" is invalid birth number."
msgstr "\"%value%\" je neplatný dátum narodenia."

msgid "\"%value%\" is invalid company IN number."
msgstr "\"%value%\" je neplatné IČO."

msgid "\"%value%\" is not an integer."
msgstr "\"%value%\" nie je celé číslo."

msgid "\"%value%\" must be at most %max%."
msgstr "\"%value%\" musí byť maximálne %max%."

msgid "\"%value%\" must be at least %min%."
msgstr "\"%value%\" musí byť minimálne %min%."

msgid "\"%value%\" is not a valid IP address."
msgstr "\"%value%\" nie je platná IP adresa."

msgid "\"%value%\" is not a number."
msgstr "\"%value%\" nie je číslo."

msgid "\"%value%\" must be less than %max%."
msgstr "\"%value%\" musí byť menej ako %max%."

msgid "\"%value%\" must be greater than %min%."
msgstr "\"%value%\" musí byť väčší ako %min%."

msgid "This session has expired. Please reload the page and try again."
msgstr "Táto relácia vypršala. Načítajte si prosím stránku a skúste to znova."

msgid "Value is too long (%max_length% characters max)."
msgstr "Hodnota je príliš dlhá (%max_length% znakov max)."

msgid "Value is too short (%min_length% characters min)."
msgstr "Hodnota je príliš krátka (%min_length% znakov min)."

msgid "\"%value%\" is invalid URL."
msgstr "\"%value%\" je neplatná URL."

msgid "\"%value%\" is invalid zip code."
msgstr "\"%value%\" je neplatné PSČ."

msgid "The captcha is not valid (%error%)."
msgstr "Výraz nie je platný (% error%)."

msgid "Unable to check the captcha from the server (%error%)."
msgstr "Nie možno overiť výraz zo servera (%error%)."

msgid "Spam bots are not welcomed here. Go away!"
msgstr "Spamovanie tu nie je vítáno. Opustite tuto webstránku!"

msgid "\"%value%\" is invalid company VAT number."
msgstr "\"%value%\" je neplatné DIČ."

msgid "Email is required."
msgstr "Zadajte email."

msgid "Email is invalid."
msgstr "Neplatný email."

msgid "Email \"%value%\" does not exist."
msgstr "Neexistujúci \"%value%\" email."

msgid "The text contains unallowed HTML tags."
msgstr "Text obsahuje nepovolené HTML tagy."

msgid "Password is required."
msgstr "Zadajte heslo"

msgid "Password is invalid."
msgstr "Neplatné heslo."

msgid "Password is too long (%max_length% characters max)."
msgstr "Heslo je príliš dlhé (%max_length% znakov max)."

msgid "Password is too short (%min_length% characters min)."
msgstr "Heslo je príliš krátke (%min_length% znakov min)."

msgid "Phone number is required."
msgstr "Zadajte telefónne číslo."

msgid "\"%value%\" is invalid phone number."
msgstr "\"%value%\" je neplatné telefónne číslo"

msgid "is empty"
msgstr "je prázdne"

msgid "from %from% to %to%"
msgstr "od %from% do %to%"

msgid "remove the current file"
msgstr "odstránite aktuálny súbor"

msgid "Available"
msgstr "K dispozícii"

msgid "Select all"
msgstr "Vyber všetko"

msgid "Unselect all"
msgstr "Odobrať všetko"

msgid "Inverse selection"
msgstr "Inverzný výber"

msgid "Filter"
msgstr "Filter"

msgid "Selected"
msgstr "Vybraný"

msgid "Class \"%value%\" is invalid."
msgstr "Trieda \"%value%\" nie je platná."

msgid "The value is invalid YAML string."
msgstr "Táto hodnota nie je neplatný YAML reťazec."

msgid "The YAML string is invalid type. Should be \"%type%\"."
msgstr "Reťazec YAML je neplatného typu. Malo by být \"%type%\"."

msgid "append"
msgstr "pridať"

msgid "prepend"
msgstr "predradiť"

msgid "replace"
msgstr "nahradiť"

msgid "no"
msgstr "nie"

msgid "yes"
msgstr "áno"

msgid "Selected title mode is invalid."
msgstr "Vybraný mód titulku nie je platný."

msgid "This value is invalid CSS class."
msgstr "Táto hodnota nie je platná CSS trieda."
